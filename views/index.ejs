<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Home Page</title>

    <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />
    <link rel="stylesheet" href="/vendor/border-box.css" type="text/css" />
    <link rel="stylesheet" href="/styles/layout.css" type="text/css" />

    <script
  src="http://code.jquery.com/jquery-3.5.1.js"
  integrity="sha256-QWo7LDvxbWT2tbbQ97B53yJnYU3WhH/C8ycbRAkjPDc="
  crossorigin="anonymous"></script>
    <script type="text/javascript" src="/scripts/app.js"></script>


    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/css/bootstrap.min.css" integrity="sha384-GJzZqFGwb1QTTN6wy59ffF1BuGJpLSa9DkKMp0DgiMDm4iYMj70gZWKYbI706tWS" crossorigin="anonymous">

    <!-- App CSS -->
    <link rel="stylesheet" href="/styles/home-page.css" type="text/css" />
  </head>

  <header>
    <%- include('partials/_header') %>
  </header>

  <body>

      <div id="scroll-text">
        <marquee>Welcome to the QuizApp!</marquee>
      </div>

      <div id="demobox">
        <marquee id="m1" class="badge badge-primary" scrollamount="2" direction="down">Tech</marquee>
        <marquee id="m2" class="badge badge-secondary" scrollamount="4" direction="down">Art</marquee>
        <marquee id="m3" class="badge badge-success" scrollamount="3" direction="down">History</marquee>
        <marquee id="m4" class="badge badge-danger" scrollamount="4" direction="down">Literature</marquee>
        <marquee id="m5" class="badge badge-warning" scrollamount="5" direction="down">Politics</marquee>
        <marquee id="m6" class="badge badge-info" scrollamount="1" direction="down">Science</marquee>
        <marquee id="m7" class="badge badge-dark" scrollamount="2" direction="down">Travel</marquee>
        <marquee id="m8" class="badge badge-light" scrollamount="3" direction="down">Sports</marquee>
        <marquee id="m9" class="badge badge-secondary" scrollamount="3" direction="down">Culture</marquee>
      </div>
      <br>
      <h3>Quiz List</h3>
      <table class="table">
      <thead>
        <tr>
          <th scope="col">Name</th>
          <th scope="col">Description</th>
          <th scope="col">URL</th>
        </tr>
      </thead>
      <tbody>
        <% for(let item in quizObj) { %>
        <tr>
          <td><%= quizObj[item].name %></td>
          <td><%= quizObj[item].description %></td>
          <td><a href="<%= quizObj[item].url %>"</a>Link</td>
        </tr
        <% } %>
      </tbody>
    </table>
  </main>
  <!-- Bootstrap JS -->


  </body>
</html>
